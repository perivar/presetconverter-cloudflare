{
  "name": "presetconverter",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler pages deploy",
    "deploy-dryrun": "npm run build && tsx ./scripts/deploy-dryrun.ts",
    "dev": "remix vite:dev",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "format-fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "start": "wrangler pages dev ./build/client",
    "test": "jest --watchAll",
    "tsc": "tsc --build --verbose",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "download-file": "tsx ./scripts/download-file.ts something.json"
  },
  "dependencies": {
